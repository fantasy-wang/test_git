git clone https...

git branch "branchname"  #make a new branch
git checkout "branchname"   #switch branch

git branch -d "branchname"   #delete local branch
git branch -a             #look all the branches


git push origin "branchname":"branchname"     #push local branch to remote branch with your define name
git push origin "branchname"      #push to remote branch


git reset --hard "commit_id"
git push -f origin "branchname"     #force to push to remote




